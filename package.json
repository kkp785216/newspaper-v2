{
    "name": "np-frontend",
    "version": "0.1.0",
    "private": true,
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "eslint --no-cache --max-warnings=3"
        ]
    },
    "scripts": {
        "dev": "env-cmd -f environments/dev.env next dev",
        "dev:prod": "env-cmd -f environments/prod.env next dev",
        "dev:staging": "env-cmd -f environments/test.env next dev",
        "build:dev": "env-cmd -f environments/dev.env next build",
        "build:staging": "env-cmd -f environments/test.env next build",
        "build:prod": "env-cmd -f environments/prod.env next build",
        "start": "next start",
        "check-types": "tsc --pretty --noEmit",
        "check-format": "prettier --check .",
        "check-lint": "eslint . --ext ts --ext tsx --ext js",
        "format": "prettier --write .",
        "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
        "prepare": "husky install",
        "lint": "next lint --no-cache --max-warnings=5"
    },
    "dependencies": {
        "env-cmd": "^10.1.0",
        "next": "13.4.19",
        "postcss": "8.4.29",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@types/node": "20.6.0",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "autoprefixer": "10.4.15",
        "eslint": "8.49.0",
        "eslint-config-next": "13.4.19",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react": "^7.33.2",
        "husky": "^8.0.0",
        "prettier": "3.0.3",
        "tailwindcss": "3.3.3",
        "typescript": "5.2.2"
    }
}
